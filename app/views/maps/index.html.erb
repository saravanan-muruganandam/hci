<h1>Listing maps</h1>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Height</th>
    <th>Width</th>
    <th>Image Path</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @maps.each do |map| %>
  <tr>
    <td><%= map.name %></td>
    <td><%= map.height %></td>
    <td><%= map.width %></td>
    <td><%= map.path %></td>
    <td><a href='http://59.78.23.86/canvas/pull.html?id=<%= map.id %>'> Editor </a> </td>
    <td> <a href='http://59.78.23.86/canvas/map.html?id=<%= map.id %>'> Change Size </a> </td>
    <td><%= link_to 'Show', map %></td>
    <td><%= link_to 'Edit', edit_map_path(map) %></td>
    <td><%= link_to 'Destroy', map, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

